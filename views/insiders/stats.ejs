<%- include("header") %>
<table class="table table-striped table-bordered">
  <thead>
    <tr>
      <th scope="col">URL</th>
      <th scope="col">Slug</th>
      <th scope="col">Visits</th>
      <th scope="col">Actions</th>
    </tr>
  </thead>
  <tbody>
    <% data.forEach((value) => { %>
    <tr>
      <td><%= value.url %></td>
      <td><%= value.slug %></td>
      <td><%= value.visited %></td>
      <td>
        <form method="GET" action="/insiders/stats/delete/<%= value._id %>">
          <button
            class="btn btn-danger"
            type="submit"
          />
            <img
              src="/images/delete.svg"
              width="20"
            />
          </button>
        </form>
      </td>
    </tr>
    <% }) %>
  </tbody>
</table>
<a class="btn btn-warning" href="/insiders/dashboard">Go back</a>

<%- include("footer") %>
